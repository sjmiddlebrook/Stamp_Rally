<main role="main">
    <div class="container">
        <h1>Stamp Locations...</h1>
        <table class="table">
            <thead>
            <th>Name</th>
            <th>Type</th>
            <th>Description</th>
            </thead>
            <tbody>
            {{#each stamps}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{type}}</td>
                    <td>{{description}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>

</main>