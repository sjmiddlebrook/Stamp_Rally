<main role="main">
    <div class="container">
        <a href="home"><h1>Stamp Rally</h1></a>
        <h3>Stamps in your Itinerary</h3>
        <table class="table">
            <thead>
            <th>Name</th>
            <th>Type</th>
            <th>Description</th>
            <th>Background</th>
            <th>Hours at Destination</th>
            <th>Rating</th>
            </thead>
            <tbody>
            {{#each stamps}}
                <tr>
                    <td>{{name}}</td>
                    <td>{{type}}</td>
                    <td>{{description}}</td>
                    <td>{{background}}</td>
                    <td>{{duration}}</td>
                    <td>{{rating}}</td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>

</main>